namespace std
{
	class unique<T>
	{
		private _allocated : T&;

		public fn finalize()
		{
			delete(_allocated);
		}

		public fn move() -> unique<T>
		{
			let result = unique<T>(_allocated);
			_allocated = null;
			return result;
		}

		public fn get() -> T&
		{
			return _allocated;
		}

		internal fn construct(value: T&)
		{
			_allocated = value;
		}
	}
}